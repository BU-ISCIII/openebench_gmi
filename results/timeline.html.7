{"label": "validateInputFormat", "times": [{"starting_time": 1557324945494, "ending_time": 1557324945530, "color":c1(1)}, {"starting_time": 1557324945530, "ending_time": 1557324945783, "color":c2(1), "label": "2.2s \/ 125.2 MB"}, {"starting_time": 1557324945783, "ending_time": 1557324947733, "color":c1(1)}]},
{"label": "getQueryIds", "times": [{"starting_time": 1557324947783, "ending_time": 1557324947833, "color":c1(2)}, {"starting_time": 1557324947833, "ending_time": 1557324948088, "color":c2(2), "label": "2.2s \/ 117.4 MB"}, {"starting_time": 1557324948088, "ending_time": 1557324949981, "color":c1(2)}]},
