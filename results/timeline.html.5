{"label": "validateInputFormat", "times": [{"starting_time": 1557325257120, "ending_time": 1557325257176, "color":c1(1)}, {"starting_time": 1557325257176, "ending_time": 1557325257427, "color":c2(1), "label": "2.3s \/ 115.2 MB"}, {"starting_time": 1557325257427, "ending_time": 1557325259425, "color":c1(1)}]},
{"label": "getQueryIds", "times": [{"starting_time": 1557325259475, "ending_time": 1557325259524, "color":c1(2)}, {"starting_time": 1557325259524, "ending_time": 1557325259783, "color":c2(2), "label": "2.3s \/ 117.3 MB"}, {"starting_time": 1557325259783, "ending_time": 1557325261816, "color":c1(2)}]},
